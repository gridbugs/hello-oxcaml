(version 4.14.1)

(install
 (run %{make} install))

(build
 (progn
  (run
   ./configure
   --prefix=%{prefix}/init_deps
   --disable-ocamldoc
   --docdir=%{prefix}/doc/ocaml
   -C)
  (run %{make} -j%{jobs})))

(source
 (fetch
  (url https://github.com/ocaml/ocaml/archive/4.14.1.tar.gz)
  (checksum
   sha256=776006e6f0b9bcfb6d9d74381c588e587432ca85562fde93bb80472a5145b028)))
